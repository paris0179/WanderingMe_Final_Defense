<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:background="@drawable/blur4"


    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:layout_marginBottom="5dp"
        android:gravity="center_vertical"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Support"
        android:id="@+id/textView1"
        android:background="@drawable/buttonshape3"
        android:textSize="25sp"
        android:typeface="normal"
        android:layout_marginTop="100dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:paddingLeft="20dp"
        android:layout_marginBottom="5dp"
        android:layout_height="60dp"
        android:paddingRight="20dp"
        android:gravity="center_vertical"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Help Center"
        android:id="@+id/textView2"
        android:background="@drawable/buttonshape3"
        android:textSize="25sp"
        android:typeface="normal"
        android:layout_below="@+id/textView1"
        android:layout_alignStart="@+id/textView1"
        android:layout_alignEnd="@+id/imageButton1"
        android:layout_alignRight="@+id/imageButton1"
        android:layout_alignLeft="@+id/textView1"/>

    <TextView
        android:layout_width="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:layout_marginBottom="5dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Terms and Conditions"
        android:id="@+id/textView3"
        android:background="@drawable/buttonshape3"
        android:textSize="25sp"
        android:typeface="normal"
        android:layout_below="@+id/textView2"
        android:layout_alignEnd="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:layout_alignRight="@+id/imageButton1"
        android:layout_alignLeft="@+id/textView1"/>

    <TextView
        android:layout_width="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:layout_height="60dp"
        android:layout_marginBottom="5dp"
        android:gravity="center_vertical"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Report"
        android:id="@+id/textView4"
        android:background="@drawable/buttonshape3"
        android:textSize="25sp"
        android:typeface="normal"
        android:layout_below="@+id/textView3"
        android:layout_alignEnd="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_alignRight="@+id/imageButton1"
        android:layout_alignLeft="@+id/textView1"/>

    <TextView
        android:layout_width="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:layout_marginBottom="5dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="About Us"
        android:id="@+id/textView5"
        android:background="@drawable/buttonshape3"
        android:textSize="25sp"
        android:typeface="normal"
        android:layout_below="@+id/textView4"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:layout_alignRight="@+id/imageButton1"
        android:layout_alignLeft="@+id/textView1"/>



    <ImageButton
        android:layout_width="55dp"
        android:src="@drawable/next"
        android:background="@drawable/buttonshape3"
        android:layout_height="30dp"
        android:id="@+id/imageButton1"
        android:layout_alignBottom="@+id/textView1"
        android:layout_alignLeft="@+id/imageButton4"
        android:layout_alignStart="@+id/imageButton4"
        android:layout_alignTop="@+id/textView1"
        android:onClick="openSupport"/>

    <ImageButton
        android:layout_width="55dp"
        android:src="@drawable/next"
        android:background="@drawable/buttonshape3"
        android:layout_height="30dp"
        android:layout_marginBottom="5dp"
        android:id="@+id/imageButton2"
        android:layout_above="@+id/textView3"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:onClick="openHelp"/>

    <ImageButton
        android:layout_width="55dp"
        android:src="@drawable/next"
        android:background="@drawable/buttonshape3"
        android:layout_height="30dp"
        android:layout_marginBottom="5dp"
        android:id="@+id/imageButton5"
        android:layout_above="@+id/textView4"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/textView2"
        android:onClick="openTerms"/>

    <ImageButton
        android:layout_width="55dp"
        android:src="@drawable/next"
        android:background="@drawable/buttonshape3"
        android:layout_height="30dp"
        android:layout_marginBottom="5dp"
        android:id="@+id/imageButton3"
        android:layout_above="@+id/textView5"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/textView3"
        android:onClick="openReport"/>

    <ImageButton
        android:layout_width="55dp"
        android:src="@drawable/next"
        android:background="@drawable/buttonshape3"
        android:layout_height="30dp"
        android:id="@+id/imageButton4"
        android:layout_alignRight="@+id/textView1"
        android:layout_alignBottom="@+id/textView5"
        android:layout_below="@+id/textView4"
        android:onClick="openAbout"/>

</RelativeLayout>
